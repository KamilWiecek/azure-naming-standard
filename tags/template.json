{
    "$schema": "https://schema.management.azure.com/schemas/2019-08-01/managementGroupDeploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "location": {
            "type": "string",
            "metadata": {
                "strongType": "location",
                "description": "The location for the policy assignment."
            }
        },
        "scope": {
            "type": "string",
            "metadata": {
                "description": "The scope for the policy definition at assignment."
            }
        },
        "assignmentScopeExclusions": {
            "type": "array",
            "defaultValue": [
            ],
            "metadata": {
                "description": "The policy's excluded scopes."
            }
        },
        "enforcementMode": {
            "type": "string",
            "allowedValues": [
                "Default",
                "DoNotEnforce"
            ],
            "defaultValue": "Default",
            "metadata": {
                "description": "The policy assignment enforcement mode. Possible values are Default and DoNotEnforce."
            }
        },
        "tagsRequiredOnResource": {
            "type": "array",
            "defaultValue": [],
            "metadata": {
                "description": "List of tags that must exist on resource."
            }
        },
        "tagsRequiredOnRg": {
            "type": "array",
            "defaultValue": [],
            "metadata": {
                "description": "List of tags that must exist on resource groups."
            }
        },
        "tagsRequiredOnSub": {
            "type": "array",
            "defaultValue": [],
            "metadata": {
                "description": "List of tags that must exist on subscription."
            }
        },
        "tagsToInheritFromRgIfMissing": {
            "type": "array",
            "defaultValue": [],
            "metadata": {
                "description": "List of tags to inherit form resource group if it doesn't exist on resource. It won't override tags having non empty value."
            }
        },
        "tagsToForceInheritFromRgForce": {
            "type": "array",
            "defaultValue": [],
            "metadata": {
                "description": "List of tags to inherit form resource group. It will override current tag value."
            }
        },
        "tagsToInheritFromSubIfMissing": {
            "type": "array",
            "defaultValue": [],
            "metadata": {
                "description": "List of tags to inherit form subscription by resources.  It won't override tags having non empty value."
            }
        },
        "tagsToInheritFromSubToRgIfMissing": {
            "type": "array",
            "defaultValue": [],
            "metadata": {
                "description": "List of tags to inherit form subscription by resource groups.  It won't override tags having non empty value."
            }
        },
        "tagsToForceInheritFromSubForce": {
            "type": "array",
            "defaultValue": [],
            "metadata": {
                "description": "List of tags to inherit form subscription by resourcess. It will override current tag value."
            }
        },
        "tagsToInheritFromSubToRgForce": {
            "type": "array",
            "defaultValue": [],
            "metadata": {
                "description": "List of tags to inherit form subscription by resource groups. It will override current tag value."
            }
        }
    },
    "variables": {
        "policySetName": "policyset-tags",
        "policyAssignmentNamePrefix": "pa-tags",
        "policyAssignmentDisplayNamePrefix": "Tags",
        "policySetDisplayName": "Tags",
        "policySetDescription": "Tagging governance. Requires tags on subscription, resource group and resource level. Automate tag inheritance.",
        "policySetCategory": "Tags",
        "policyDefinitions": {
            "requireTagOnResource": "/providers/Microsoft.Authorization/policyDefinitions/871b6d14-10aa-478d-b590-94f262ecfa99",
            "requireTagOnRg": "/providers/Microsoft.Authorization/policyDefinitions/96670d01-0a4d-4649-9c89-2d3abc0a5025",
            "inheritTagFromRgIfMissing": "/providers/Microsoft.Authorization/policyDefinitions/ea3f2387-9b95-492a-a190-fcdc54f7b070",
            "inheritTagFromRgForce": "/providers/Microsoft.Authorization/policyDefinitions/cd3aa116-8754-49c9-a813-ad46512ece54",
            "inheritTagFromSubIfMissing": "/providers/Microsoft.Authorization/policyDefinitions/40df99da-1232-49b1-a39a-6da8d878f469",
            "inheritTagFromSubForce": "/providers/Microsoft.Authorization/policyDefinitions/b27a0cbd-a167-4dfa-ae64-4337be671140"
        },
        "customPolicyDefinitions": {
            "requireTagOnSub": {
                "name": "16f39d7a-e1cf-4dc3-8eec-6ca2885656e5",
                "policyDefinitionId": "[concat(parameters('scope'),'/providers/Microsoft.Authorization/policyDefinitions/16f39d7a-e1cf-4dc3-8eec-6ca2885656e5') ]",
                "displayName": "Require a tag on subscription",
                "description": "Enforces existence of a tag on subscription.",
                "category": "Tags"
            },
            "inheritTagFromSubToRgIfMissing": {
                "name": "5454edb0-35c5-4d43-8598-713cacd0002f",
                "policyDefinitionId": "[concat(parameters('scope'),'/providers/Microsoft.Authorization/policyDefinitions/5454edb0-35c5-4d43-8598-713cacd0002f') ]",
                "displayName": "Inherit a tag from the subscription to resource group if missing",
                "description": "Adds the specified tag with its value on the resource group from the containing subscription when any resource group missing this tag is created or updated. Existing resource groups can be remediated by triggering a remediation task. If the tag exists with a different value it will not be changed.",
                "category": "Tags"
            },
            "inheritTagFromSubToRgForce": {
                "name": "15b06457-8105-4e8b-9721-cf494aaa3cf9",
                "policyDefinitionId": "[concat(parameters('scope'),'/providers/Microsoft.Authorization/policyDefinitions/15b06457-8105-4e8b-9721-cf494aaa3cf9') ]",
                "displayName": "Inherit a tag from the subscription to resource group",
                "description": "Adds or replaces the specified tag and value on the resource group from the containing subscription when any resource group is created or updated. Existing resource groups can be remediated by triggering a remediation task.",
                "category": "Tags"
            },
            "costCenterTagAllowedValues": {
                "name": "c53d67ba-234a-40d8-ac01-60bf27477748",
                "policyDefinitionId": "[concat(parameters('scope'),'/providers/Microsoft.Authorization/policyDefinitions/c53d67ba-234a-40d8-ac01-60bf27477748') ]",
                "displayName": "Require specific values ​​of the costCenter tag",
                "description": "Case sensitive validation of the costCenter tag value. The valid values ​​are 'ABC', 'BCD', or 'CDE'. If resource group or resource do not have the correct value for this tag set, then create / update request will be deny.",
                "category": "Tags"
            }
        },
        "copy": [
            {
                "name": "requiredTagOnResourcePolicies",
                "count": "[length(parameters('tagsRequiredOnResource'))]",
                "input": {
                    "policyDefinitionId": "[variables('policyDefinitions').requireTagOnResource]",
                    "parameters": {
                        "tagName": {
                            "value": "[parameters('tagsRequiredOnResource')[copyIndex('requiredTagOnResourcePolicies')]]"
                        }
                    }
                }
            },
            {
                "name": "requiredTagOnRgPolicies",
                "count": "[length(parameters('tagsRequiredOnRg'))]",
                "input": {
                    "policyDefinitionId": "[variables('policyDefinitions').requireTagOnRg ]",
                    "parameters": {
                        "tagName": {
                            "value": "[parameters('tagsRequiredOnRg')[copyIndex('requiredTagOnRgPolicies')]]"
                        }
                    }
                }
            },
            {
                "name": "requiredTagOnSubPolicies",
                "count": "[length(parameters('tagsRequiredOnSub'))]",
                "input": {
                    "policyDefinitionId": "[variables('customPolicyDefinitions').requireTagOnSub.policyDefinitionId ]",
                    "parameters": {
                        "tagName": {
                            "value": "[parameters('tagsRequiredOnSub')[copyIndex('requiredTagOnSubPolicies')]]"
                        }
                    }
                }
            },
            {
                "name": "tagsToInheritFromRgIfMissingPolicies",
                "count": "[length(parameters('tagsToInheritFromRgIfMissing'))]",
