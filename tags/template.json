                        "tagName": {
                            "value": "[parameters('tagsToInheritFromRgIfMissing')[copyIndex('tagsToInheritFromRgIfMissingPolicies')]]"
                        }
                    }
                }
            },
            {
                "name": "tagsToInheritFromRgForcePolicies",
                "count": "[length(parameters('tagsToForceInheritFromRgForce'))]",
                "input": {
                    "policyDefinitionId": "[variables('policyDefinitions').inheritTagFromRgForce ]",
                    "parameters": {
                        "tagName": {
                            "value": "[parameters('tagsToForceInheritFromRgForce')[copyIndex('tagsToInheritFromRgForcePolicies')]]"
                        }
                    }
                }
            },
            {
                "name": "inheritTagFromSubsIfMissingPolicies",
                "count": "[length(parameters('tagsToInheritFromSubIfMissing'))]",
                "input": {
                    "policyDefinitionId": "[variables('policyDefinitions').inheritTagFromSubIfMissing ]",
                    "parameters": {
                        "tagName": {
                            "value": "[parameters('tagsToInheritFromSubIfMissing')[copyIndex('inheritTagFromSubsIfMissingPolicies')]]"
                        }
                    }
                }
            },
            {
                "name": "inheritTagFromSubsToRgIfMissingPolicies",
                "count": "[length(parameters('tagsToInheritFromSubToRgIfMissing'))]",
                "input": {
                    "policyDefinitionId": "[variables('customPolicyDefinitions').inheritTagFromSubToRgIfMissing.policyDefinitionId ]",
                    "parameters": {
                        "tagName": {
                            "value": "[parameters('tagsToInheritFromSubToRgIfMissing')[copyIndex('inheritTagFromSubsToRgIfMissingPolicies')]]"
                        }
                    }
                }
            },
            {
                "name": "inheritTagFromSubForcePolicies",
                "count": "[length(parameters('tagsToForceInheritFromSubForce'))]",
                "input": {
                    "policyDefinitionId": "[variables('customPolicyDefinitions').requireTagOnSub.policyDefinitionId ]",
                    "parameters": {
                        "tagName": {
                            "value": "[parameters('tagsToForceInheritFromSubForce')[copyIndex('inheritTagFromSubForcePolicies')]]"
                        }
                    }
                }
            },
            {
                "name": "inheritTagFromSubsToRgForcePolicies",
                "count": "[length(parameters('tagsToInheritFromSubToRgForce'))]",
                "input": {
                    "policyDefinitionId": "[variables('customPolicyDefinitions').inheritTagFromSubToRgForce.policyDefinitionId ]",
                    "parameters": {
                        "tagName": {
                            "value": "[parameters('tagsToInheritFromSubToRgForce')[copyIndex('inheritTagFromSubsToRgForcePolicies')]]"
                        }
                    }
                }
            }
        ],
        "policySetPolicyDefinitions": "[union( json(concat('[{\"policyDefinitionId\": \"', variables('customPolicyDefinitions').costCenterTagAllowedValues.policyDefinitionId,'\"}]' ) ), variables('requiredTagOnResourcePolicies'), variables('requiredTagOnRgPolicies'), variables('requiredTagOnSubPolicies'), variables('tagsToInheritFromRgIfMissingPolicies'), variables('tagsToInheritFromRgForcePolicies'), variables('inheritTagFromSubsIfMissingPolicies'), variables('inheritTagFromSubForcePolicies'), variables('inheritTagFromSubsToRgForcePolicies'), variables('inheritTagFromSubsToRgIfMissingPolicies') ) ]",
